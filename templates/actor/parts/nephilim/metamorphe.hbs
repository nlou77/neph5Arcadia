                    class="item-checkbox" 
                    name="data.metamorphoses.[0]"
                    type="checkbox"
                    {{checked data.metamorphe.metamorphoses.[0]}}
                    data-dtype="Boolean"/>
                <i class="far fa-circle unchecked"></i>
                <i class="fas fa-circle checked"></i>
            </label>

            {{#with (getItem data.metamorphe.refid)}}
            <label class="item-name">{{data.data.metamorphoses.[0].name}}</label>
            {{/with}}

