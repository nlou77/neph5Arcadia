<div class="section">

    <div class="sectionRow">

        <div class="circle">
            <div class="circleContent">{{metamorphe}}</div>
        </div>

        <div class="sectionTitle">
            {{#if data.metamorphe.refid}}
                {{#with (getItem data.metamorphe.refid)}}
                <span class="sectionName">{{name}}</span>
                {{/with}}
            {{else}}
                <span class="sectionName">{{localize "ITEM.TypeMetamorphe"}}</span>
            {{/if}}
        </div>

    </div>

