                    <a class="roll-ka" data-element="terre" data-scope="actor"><i class="fas fa-dice-d20"></i></a>
                </div>
            </div>

        </div>

        <div class="header">

            <img class="puce"/>

            <img class="separator"/>

            <span class="name">{{localize "NEPH5E.chutes"}}</span>

        </div>

        <div class="chutes">

