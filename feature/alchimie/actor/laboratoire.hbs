            <select name="actor.system.alchimie.constructs.creuset.degre">
            {{#select degre}}
                {{#each ../cercles as |name label|}}
                <option value="{{label}}">{{localize name}}</option>
                {{/each}}
            {{/select}}
            </select>

            <div class="elements">

                <img class="pentacle" src="systems/neph5e/assets/core/pentacle.webp"/>

                <div class="element tooltip air">
                    <img class="circle"/>
                    <input
                        class="degre"
                        name="actor.system.alchimie.constructs.creuset.air"
                        type="text"
                        value="{{actor.system.alchimie.constructs.creuset.air}}"
                        data-dtype="Number"/>
