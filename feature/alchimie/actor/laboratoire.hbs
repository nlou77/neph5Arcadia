<div class="laboratoire">

    <div class="laboratoires">

        <label class="zone">...</label>

        <ol class="menu">

            {{#each actor.laboratoires as |item|}}         
            <li data-id="{{item.id}}">
               <label class="name">{{item.name}}</label>
            </li>
            {{/each}}

